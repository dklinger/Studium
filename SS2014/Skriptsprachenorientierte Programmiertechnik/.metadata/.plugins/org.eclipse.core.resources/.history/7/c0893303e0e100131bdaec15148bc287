<?php

echo fibunaci(5);

function fibunaci($stelle)
{
	$akt = 0;
	$erg = 0;
	$zahl = 1;
	while($akt < $stelle)
	{
		$erg = $zahl + $erg;
		
		
		$erg = $erg + $zahl;
		
		$akt++;
		$tmp = $start;
		$zahl = $zahl + $start;
		$start = $tmp + $zahl;	
		
		echo "$tmp	$zahl	$start\r\n";
	}
	
	//0+1=1  1+1=2  1+2=3 
	
	
	
	//1		1	2
	//2		3	5
	//5		8	13
	
	return $zahl;
	
	//1 1 2 3 5 8 13 21 34
}

?>