<?php

echo fibunaci(5);

function fibunaci($stelle)
{
	$akt = 0;
	$start = 0;
	$zahl = 1;
	while($akt < $stelle)
	{
		$akt++;
		$akt++;
		$tmp = $start;
		$zahl = $zahl + $start;
		$start = $tmp + $zahl;	
		
		echo "$tmp	$zahl	$start\r\n";
	}
	
	//1		1	2
	//2		3	5
	//5		8	13
	
	return $zahl;
	
	//1 1 2 3 5 8 13 21 34
}

?>