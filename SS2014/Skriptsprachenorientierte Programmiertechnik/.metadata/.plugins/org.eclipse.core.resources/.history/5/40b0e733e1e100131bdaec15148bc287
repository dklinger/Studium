<?php

$starttime = microtime(true);
echo fibunaci(999) ." ";
$endtime = microtime(true);

echo $endtime-$starttime;

function fibunaci($stelle)
{
//	$akt = 0;
	$erg = 1;
	$start = 0;
	/*while($akt < $stelle)
	{
		$akt++;
		$tmp = $erg;
		$erg = $start + $erg;
		$start = $tmp;
	}*/
	
	for($i = 0; $i < $stelle; $i++)
	{
		$tmp = $erg;
		$erg = $start + $erg;
		$start = $tmp;
	}
	
	return $erg;
}

?>